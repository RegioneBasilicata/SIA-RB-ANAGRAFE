			<param-name>IRIDE_ID_SESSIONATTR</param-name>
			<param-value>identita</param-value>
		</init-param>
		<!--
			elenco delle pagine di servizio che non vanno gestite dal filtro
		-->
		<init-param>
			<param-name>NO_CHECK_PAGE</param-name>
			<param-value/>
		</init-param>
	</filter>

