	<!-- Statrace 20/05/2009 cz     																  -->
	<!-- ############################################################################################ -->
	<!-- 
		LoggerName: nome del logger che deve utilizzare il componente JSTATRACE per il 
		logging del suo funzionamento (da standard deve coincidere con il nome del 
		pacchetto deployato)
	-->
	<context-param>		
		<param-name>LoggerName</param-name>
		<param-value>smrgaaweb</param-value>
	</context-param>
	<!--
		TraceLoggerName: nome del logger che deve utilizzare il componente JSTATRACE 
		per la generazione delle tracce statistiche (uno diverso per ogni applicazione)
	-->
	<context-param>		
		<param-name>TraceLoggerName</param-name>
		<param-value>STTRACE_smrgaaweb</param-value>
	</context-param>
	<!--
		Portale: nome del portale sul quale l applicazione viene ospitata (nel caso di 
		applicazioni multiportale, scegliere uno qualunque dei portali ospitanti, 
		sono disponibili API per la modifica del valore a runtime)
	-->
	<context-param>		
		<param-name>Portale</param-name>
		<param-value></param-value>
	</context-param>
	<!--
		DFTemplate:Template timestamp
	-->
	<context-param>		
		<param-name>DFTemplate</param-name>
		<param-value>yyyy/MM/dd - HH:mm:ss</param-value>
	</context-param>
	<!--
		Keystore: path del file di configurazione in cui viene conservata la chiave 
		usata per criptare il Codice Fiscale
	-->
	<context-param>		
		<param-name>Keystore</param-name>
		<!--param-value>/home/apps/tstbeata09/barreweb/masterkey.properties</param-value-->
		<param-value></param-value>
	</context-param>
	<!--
		ApplName: Nome dell applicazione web sottoposta a tracciamento (il valore 
		di tale parametro corrisponde al contenuto del tag <APPSERVERID> del tracefile di default)
	-->
	<context-param>		
		<param-name>ApplName</param-name>
		<param-value>smrgaaweb</param-value>
	</context-param>
